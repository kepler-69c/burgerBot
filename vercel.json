{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "dest": "app.py"
        },
        {
            "src": "/",
            "dest": "app.py"
        },
        {
            "src": "/api/change/(:token)/(:sending)",
            "dest": "app.py"
        }
    ],
    "crons": [
        {
            "path": "/api",
            "schedule": "00 05 * * 1-5"
        }
    ]
}